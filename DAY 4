
class a:
    
    x=input("string:")
    y=int(input("integer1:"))
    z=int(input("integer2:"))
    def b(self,x,y):
        self.x=x
        self.y=y
        print(x[::-1])
        print(y**2)
    def display_results(self,x,y,z):
        self.x=x
        self.y=y
        self.z=z
        print(len(x))
        print(y%z)
obj=a()
obj.b(obj.x,obj.y)
obj.display_results(obj.x,obj.y,obj.z)
